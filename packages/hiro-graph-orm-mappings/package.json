{
  "name": "hiro-graph-orm-mappings",
  "version": "0.0.4-0",
  "repository": "arago/hiro-graph-js",
  "description": "a set of mapping definitions useful across OGIT apps using `hiro-graph-orm`",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf lib/* && babel src/ --out-dir lib/ --source-maps",
    "watch": "babel --watch src/ --out-dir lib/",
    "prepublish": "npm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@arago/eslint-config-arago": "github:arago/eslint-config-arago",
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^2.11.1"
  },
  "eslintConfig": {
    "extends": "@arago/eslint-config-arago"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "sourceMaps": true,
    "env": {
      "production": {
        "sourceMaps": false
      }
    }
  }
}
