{
  "name": "hiro-graph-client",
  "version": "0.0.0",
  "description": "Javascript client for HIRO Graph REST API. Works in both node and browser.",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src/",
    "test": "mocha --require babel-register --compilers js:source-map-support/register",
    "build": "rm -rf lib/* && babel src/ --out-dir lib/  --source-maps",
    "watch": "babel --watch src/ --out-dir lib/",
    "prepublish": "npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "websocket": "^1.0.22"
  },
  "devDependencies": {
    "@arago/eslint-config-arago": "github:arago/eslint-config-arago",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-runtime": "^6.9.2",
    "eslint": "^2.13.0"
  },
  "eslintConfig": {
    "extends": "@arago/eslint-config-arago"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-object-rest-spread"
    ],
    "sourceMaps": true,
    "env": {
      "production": {
        "sourceMaps": false
      }
    }
  }
}